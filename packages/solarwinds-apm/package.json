{
  "name": "solarwinds-apm",
  "version": "14.0.0-pre.7",
  "description": "OpenTelemetry-based SolarWinds APM library",
  "license": "Apache-2.0",
  "contributors": [
    "Raphaël Thériault <raphael.theriault@solarwinds.com>"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/solarwindscloud/solarwinds-apm-js.git",
    "directory": "packages/solarwinds-apm"
  },
  "bugs": {
    "url": "https://github.com/solarwindscloud/solarwinds-apm-js/issues"
  },
  "keywords": [
    "tracing",
    "instrumentation",
    "performance",
    "monitoring",
    "profiling",
    "debugging",
    "analytics",
    "metrics",
    "apm",
    "otel",
    "opentelemetry",
    "telemetry",
    "solarwinds"
  ],
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/es/index.es.js",
      "require": "./dist/cjs/index.cjs.js"
    },
    "./loader": {
      "import": "./dist/es/hooks.es.js"
    }
  },
  "main": "./dist/cjs/index.cjs.js",
  "files": [
    "./src/",
    "./dist/",
    "./CONFIGURATION.md"
  ],
  "publishConfig": {
    "provenance": true
  },
  "scripts": {
    "build": "rollup -c",
    "lint": "prettier --check . && eslint . --max-warnings=0",
    "lint:fix": "eslint --fix . && prettier --write .",
    "publish": "node ../../scripts/publish.js",
    "test": "swtest -p test/tsconfig.json -c src"
  },
  "dependencies": {
    "@opentelemetry/core": "~1.17.0",
    "@opentelemetry/instrumentation": "~0.43.0",
    "@opentelemetry/resources": "~1.17.0",
    "@opentelemetry/sdk-metrics": "~1.17.0",
    "@opentelemetry/sdk-trace-node": "~1.17.0",
    "@opentelemetry/semantic-conventions": "~1.17.0",
    "@solarwinds-apm/bindings": "workspace:^",
    "@solarwinds-apm/instrumentations": "workspace:^",
    "@solarwinds-apm/module": "workspace:^",
    "@solarwinds-apm/sdk": "workspace:^",
    "semver": "^7.5.4",
    "zod": "^3.22.4"
  },
  "peerDependencies": {
    "@opentelemetry/api": "^1.3.0",
    "@opentelemetry/exporter-metrics-otlp-grpc": ">=0.34.0",
    "@opentelemetry/exporter-trace-otlp-grpc": ">=0.34.0"
  },
  "peerDependenciesMeta": {
    "@opentelemetry/api": {
      "optional": false
    },
    "@opentelemetry/exporter-metrics-otlp-grpc": {
      "optional": true
    },
    "@opentelemetry/exporter-trace-otlp-grpc": {
      "optional": true
    }
  },
  "devDependencies": {
    "@opentelemetry/api": "^1.3.0",
    "@opentelemetry/exporter-metrics-otlp-grpc": "~0.43.0",
    "@opentelemetry/exporter-trace-otlp-grpc": "~0.43.0",
    "@solarwinds-apm/eslint-config": "workspace:^",
    "@solarwinds-apm/rollup-config": "workspace:^",
    "@solarwinds-apm/test": "workspace:^",
    "@types/node": "^20.8.0",
    "@types/semver": "^7.5.3",
    "eslint": "^8.50.0",
    "prettier": "^3.0.3",
    "rollup": "patch:rollup@npm%3A4.0.2#~/.yarn/patches/rollup-npm-4.0.2-c9dd2d43a6.patch",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=16.13.0"
  },
  "stableVersion": "13.0.0"
}
