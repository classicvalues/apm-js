{
  "name": "swotel",
  "private": true,
  "license": "Apache-2.0",
  "packageManager": "yarn@3.5.1",
  "workspaces": [
    "examples",
    "examples/*",
    "packages/*",
    "packages/*/npm/*",
    "scripts"
  ],
  "scripts": {
    "build": "turbo run build",
    "docker": "node scripts/docker.js",
    "example": "node scripts/example.js",
    "lint": "prettier --check *.json *.md && turbo run lint",
    "lint:fix": "prettier --write *.json *.md && turbo run lint:fix",
    "oboe": "node scripts/oboe.js",
    "test": "turbo run test",
    "vscode": "yarn dlx @yarnpkg/sdks vscode && node scripts/vscode.js && prettier --write .vscode"
  },
  "devDependencies": {
    "eslint": "^8.41.0",
    "prettier": "^2.8.8",
    "turbo": "^1.9.9",
    "typescript": "^5.0.4"
  }
}
