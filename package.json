{
  "name": "@solarwinds-apm/solarwinds-apm",
  "private": true,
  "license": "Apache-2.0",
  "packageManager": "yarn@4.0.0-rc.48",
  "workspaces": [
    "examples",
    "examples/*",
    "packages/*",
    "packages/bindings/npm/*",
    "scripts"
  ],
  "scripts": {
    "build": "turbo run build",
    "docker": "node scripts/docker.js",
    "example": "node scripts/example.js",
    "lint": "prettier --check *.json *.md .github && turbo run lint",
    "lint:fix": "prettier --write *.json *.md .github && turbo run lint:fix",
    "plugins:upgrade": "yarn plugin import version && yarn plugin import interactive-tools",
    "publish": "turbo run lint && turbo run build && turbo run publish",
    "test": "turbo run test",
    "version:stable": "node scripts/version.js",
    "version:pre": "node scripts/version.js pre",
    "vscode": "yarn dlx @yarnpkg/sdks vscode && node scripts/vscode.js && prettier --write .vscode"
  },
  "devDependencies": {
    "prettier": "^3.0.0",
    "turbo": "^1.10.10"
  },
  "dependenciesMeta": {
    "prettier": {
      "unplugged": true
    }
  }
}
