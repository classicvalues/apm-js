{
  "name": "@solarwinds-apm/solarwinds-apm",
  "private": true,
  "license": "Apache-2.0",
  "packageManager": "yarn@3.6.0",
  "workspaces": [
    "examples",
    "examples/*",
    "packages/*",
    "packages/*/npm/*",
    "scripts"
  ],
  "scripts": {
    "build": "turbo run build",
    "docker": "node scripts/docker.js",
    "example": "node scripts/example.js",
    "lint": "prettier --check *.json *.md && turbo run lint",
    "lint:fix": "prettier --write *.json *.md && turbo run lint:fix",
    "plugins:upgrade": "yarn plugin import version && yarn plugin import interactive-tools",
    "test": "turbo run test",
    "version:stable": "node scripts/version.js",
    "version:pre": "node scripts/version.js pre",
    "vscode": "yarn dlx @yarnpkg/sdks vscode && node scripts/vscode.js && prettier --write .vscode"
  },
  "devDependencies": {
    "eslint": "^8.44.0",
    "prettier": "^3.0.0",
    "turbo": "^1.10.7",
    "typescript": "^5.1.6"
  }
}
