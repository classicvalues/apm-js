{
  "name": "swotel",
  "private": true,
  "packageManager": "yarn@3.4.1",
  "workspaces": [
    "examples",
    "examples/*",
    "packages/*",
    "packages/bindings/npm/*",
    "scripts"
  ],
  "scripts": {
    "build": "turbo run build",
    "docker": "node scripts/docker.js",
    "example": "node scripts/example.js",
    "lint": "prettier --check *.json .clang-format && turbo run lint",
    "oboe": "node scripts/oboe.js",
    "test": "turbo run test",
    "test:only": "turbo run test --only",
    "vscode": "sdks vscode && node scripts/vscode.js && prettier --write .vscode"
  },
  "devDependencies": {
    "@yarnpkg/sdks": "^3.0.0-rc.34",
    "eslint": "^8.30.0",
    "prettier": "^2.8.1",
    "turbo": "^1.6.3",
    "typescript": "^4.9.4"
  }
}
