{
  "name": "swotel",
  "private": true,
  "packageManager": "yarn@3.5.0",
  "workspaces": [
    "examples",
    "examples/*",
    "packages/*",
    "packages/bindings/npm/*",
    "scripts"
  ],
  "scripts": {
    "build": "turbo run build",
    "docker": "node scripts/docker.js",
    "example": "node scripts/example.js",
    "lint": "prettier --check *.json .clang-format && turbo run lint",
    "oboe": "node scripts/oboe.js",
    "test": "turbo run test",
    "vscode": "yarn dlx @yarnpkg/sdks vscode && node scripts/vscode.js && prettier --write .vscode"
  },
  "devDependencies": {
    "eslint": "^8.37.0",
    "prettier": "^2.8.7",
    "turbo": "^1.8.8",
    "typescript": "^5.0.3"
  }
}
